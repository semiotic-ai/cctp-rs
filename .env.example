# CCTP Testnet Configuration
# Copy this file to .env and fill in your values

# Required: Your wallet private key (include the 0x prefix)
TESTNET_PRIVATE_KEY=0x...

# Required: Alchemy API key for RPC access
TESTNET_API_KEY=your-alchemy-api-key

# Optional: Override default Base Sepolia RPC
# BASE_SEPOLIA_RPC_URL=https://base-sepolia.g.alchemy.com/v2/YOUR_KEY

# Optional: Override default Arbitrum Sepolia RPC
# ARBITRUM_SEPOLIA_RPC_URL=https://arb-sepolia.g.alchemy.com/v2/YOUR_KEY
